{
  "name": "logify",
  "displayName": "logify",
  "description": "A simple way to effortlessly print logs to the VS Code console with this user-friendly extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "logify.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "logify.addConsole",
        "title": "Add console.dir()"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0"
  },
  "dependencies": {
    "camelcase": "^6.3.0"
  }
}